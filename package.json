{
  "name": "my-webpack-project",
  "version": "1.0.0",
  "description": "My webpack project",
  "private": true,
  "scripts": {
    "start": "webpack serve",
    "build": "webpack",
    "test": "jest",
    "start:e2e": "NODE_ENV=e2e nyc --silent npm start",
    "cover:merge": "cp -f jest-coverage/coverage-final.json .nyc_output",
    "cover:report": "nyc report --reporter=lcov --reporter=text-summary",
    "cover:codecov": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "cypress": "cypress run",
    "e2e": "start-server-and-test start:e2e http://localhost:8080 cypress"
  },
  "dependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-flow": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "axios": "^0.21.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "copy-webpack-plugin": "^6.2.1",
    "css-loader": "^5.0.0",
    "css-minimizer-webpack-plugin": "^1.1.5",
    "eslint": "^7.12.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^4.5.0",
    "inline-chunk-html-plugin": "^1.1.1",
    "mini-css-extract-plugin": "^1.2.1",
    "react": "^17.0.1",
    "react-dev-utils": "^11.0.0",
    "react-dom": "^17.0.1",
    "react-query": "^2.26.2",
    "react-redux": "^7.2.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "shallowequal": "^1.1.0",
    "style-loader": "^2.0.0",
    "styled-components": "^5.2.0",
    "terser-webpack-plugin": "^5.0.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.3.2",
    "webpack-cli": "^4.1.0"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.8.2",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "babel-jest": "^26.6.1",
    "camelcase": "^6.2.0",
    "codecov": "^3.8.0",
    "cypress": "^5.5.0",
    "husky": ">=4",
    "jest": "26.6.1",
    "jest-environment-jsdom-fourteen": "1.0.1",
    "jest-resolve": "26.6.1",
    "jest-watch-typeahead": "0.6.1",
    "lint-staged": ">=10",
    "msw": "^0.21.3",
    "npm-check-updates": "^9.2.3",
    "nyc": "^15.1.0",
    "prettier": "^2.1.2",
    "react-app-polyfill": "^2.0.0",
    "react-test-renderer": "^17.0.1",
    "start-server-and-test": "^1.11.5",
    "webpack-dev-server": "^3.11.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": "eslint --cache --fix",
    "*.{js,jsx,css,md}": "prettier --write"
  }
}
